
<style>
  h1{
    text-align: center;
    font-weight: 700;
    font-size: 2em;
  }
</style>


  <div class= container>
    <h1>Flashcards for this Deck:</h1>
  </div>
<hr>
  <div class="container">
      <a href="/deck/{{deck_id}}/newcard" class="btn btn-success btn-lg btn-block">Add a New Card</a>
    <br>
      <a href="/deck/{{deck_id}}/test" class="btn btn-secondary btn-lg btn-block">Test Your Skills</a>
    <br>
      <a href="/deck" class="btn btn-info btn-lg btn-block">Back to All Decks</a>

<hr>

<h4>LET'S REVIEW:</h4>
    {{!-- create a card for each card in deck --}}
<div class="container">
  <div class="row">

    

  {{#each cards}} 
    <div class="col-sm-6">
    <div class="card" style="width: 18rem;">
       {{#if imgurl}}
          <img class="card-img-top" src="{{imgurl}}" alt="Card image cap">
       {{else}}
        <img class="card-img-top" src="data:image/jpeg;base64," alt="Card image cap">
        {{/if}}
       <div class="card-body">
          <h5 class="card-title">{{question}}</h5>
          <p class="card-text">{{answer}}</p>
    
          <a href="/delete/card/{{card_id}}" class= "btn btn-danger"><i class="fas fa-trash-alt"></i></a>
        </div>
        
    </div><br>
    </div>
    

    {{/each}}
  </div>
</div>  
{{!-- Buttons --}}




